function acronymMaker (str) {
    var words, acronym, nextWord;

    words = str.split(' ');
    acronym= "";
    index = 0;
    while (index<words.length) {
            nextWord = words[index];
            acronym = acronym + nextWord.charAt(0);
            index = index + 1 ;
    }
    return acronym;
}
acronymMaker('Cali Without Water');
