function owassoRams(n){
  var arr = []
  for (var i = 1; i <= n; i++ ) {
    if ( i%3 === 0 && i%5 === 0 ) {
      arr.push("OwassoRams");
    } else if ( i%3 === 0 ) {
      arr.push("Owasso" );
    } else if ( i%5 === 0 ) {
      arr.push("Rams");
    } else {
      arr.push(i);
    }
  }
  return arr
}

var output = owassoRams(15);
console.log(output);
